<h3> {{namepage}}</h3>
{{#if last_name}}
<h2>{{last_name}}</h2>
{{else}}

{{/if}}
<div class="d-flex justify-content-evenly">
{{#each alumnos}}
    <div class="card" style="width: 15rem">
        <img src={{this.photo}} class="card-img-top" style="width: 15rem;height:8rem">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text"> {{this.description}} </p>
       <a href="#" class="btn agregar-carrito"> Agregar al carrito </a>
        </div>
    </div>
{{/each}}
</div>