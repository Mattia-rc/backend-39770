<div class="d-flex justify-content-evenly">
    {{#each products}}
        <div class="card" style="width: 15rem">
            <img src={{this.photo}} class="card-img-top" style="width: 15rem;height:8rem">
            <div class="card-body">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text"> ${{this.price}} </p>
                <a href="#" class="btn agregar-carrito"> Agregar al carrito </a>
            </div>
        </div>
    {{/each}}
</div>